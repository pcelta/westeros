{
    "name": "pcelta/westeros",
    "description": "A Class structure based on Westeros",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "unit tests",
        "unit",
        "phpunit",
        "westeros"
    ],
    "authors": [
        {
            "name": "Pedro Ribeiro da Silva Neto",
            "email": "pcelta.ribeiro@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.0",
        "friendsofphp/php-cs-fixer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Westeros\\": "src/Westeros"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/unit/"
        }
    }
}